{
	"figmaUrl": "",
	"figmaToken": "",
	"figmaPage": "Design Tokens",
	"format": "js",
	"apiBaseUrl": "https://api.figma.com/v1/files/",
	"figmaJson": false,
	"figmaTokens": true,
	"outputColorFormat": "rgba",
	"outputCSSUnit": "em",
	"outputNameFormat": "kebab",
	"tokens": [
		{
			"frameName": "time",
			"group": true,
			"name": "animation",
			"path": "characters"
		},
		{
			"frameName": "colors",
			"name": "color",
			"path": "fills[0].color",
			"style": true,
			"styleKey": "fill",
			"processValue": "color"
		},
		{
			"frameName": "shadows",
			"name": "shadow",
			"path": "effects[0]",
			"style": true,
			"styleKey": "effect",
			"processValue": "shadow"
		}
	]
}
